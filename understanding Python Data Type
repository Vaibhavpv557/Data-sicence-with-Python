def main():
    # Creating a list
    my_list = [1, 2, 3, 4, 5]
    print("Original list:", my_list)
    
    # Adding elements to the list
    my_list.append(6)
    print("List after adding element:", my_list)
    
    # Modifying an element in the list
    my_list[2] = 30
    print("List after modifying element at index 2:", my_list)
    
    # Removing an element from the list
    my_list.remove(4)
    print("List after removing element 4:", my_list)
    
    # Creating a dictionary
    my_dict = {'a': 1, 'b': 2, 'c': 3}
    print("\nOriginal dictionary:", my_dict)
    
    # Adding a new key-value pair to the dictionary
    my_dict['d'] = 4
    print("Dictionary after adding key 'd':", my_dict)
    
    # Modifying the value of an existing key in the dictionary
    my_dict['b'] = 20
    print("Dictionary after modifying value of key 'b':", my_dict)
    
    # Removing a key-value pair from the dictionary
    del my_dict['a']
    print("Dictionary after removing key 'a':", my_dict)
    
    # Creating a set
    my_set = {1, 2, 3, 4, 5}
    print("\nOriginal set:", my_set)
    
    # Adding an element to the set
    my_set.add(6)
    print("Set after adding element:", my_set)
    
    # Removing an element from the set
    my_set.remove(3)
    print("Set after removing element 3:", my_set)
    
    # Trying to add duplicate element to set (won't change the set)
    my_set.add(6)
    print("Set after trying to add duplicate element:", my_set)

if __name__ == "__main__":
    main()
